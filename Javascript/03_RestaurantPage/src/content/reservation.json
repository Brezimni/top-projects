{
    "title": {
        "tag": "h2",
        "text": "Book a table!"
    },
    "reservation-form": {
        "tag": "form",
        "classes": ["reservation"],
        "input1": {
            "tag": "input",
            "type": "text",
            "placeholder": "Tell us your name",
            "required": "true"
        },
        "input2": {
            "tag": "input",
            "type": "date",
            "name": "date",
            "required": "true"
        },
        "input3": {
            "tag": "input",
            "type": "time",
            "name": "time",
            "required": "true",
            "min": "09:00",
            "max": "21:00"
        },
        "button": {
            "tag": "button",
            "type": "submit",
            "text": "Send"
        }
    },
    "reservation-success": {
        "classes": ["rsuccess", "hidden"],
        "svg": {
        "tag": "svg",
        "viewBox": "0 0 130.2 130.2",
        "circle": {
            "tag": "circle",
            "classes": ["path", "circle"],
            "fill": "none",
            "stroke": "#73AF55",
            "stroke-width": "6",
            "stroke-miterlimit": "10",
            "cx": "65.1",
            "cy": "65.1",
            "r": "62.1"
        },
        "check-mark": {
            "tag": "polyline",
            "classes": ["path", "check"],
            "fill": "none",
            "stroke": "#73AF55",
            "stroke-width": "6",
            "stroke-linecap": "round",
            "stroke-miterlimit": "10",
            "points": "100.2,40.2 51.5,88.8 29.8,67.5"
        }}
    },
    "reservation-fail": {
        "classes": ["rfail", "hidden"],
        "svg": {
        "tag": "svg",
        "viewBox": "0 0 130.2 130.2",
        "circle": {
            "tag": "circle",
            "classes": ["path", "circle"],
            "fill": "none",
            "stroke": "#D06079",
            "stroke-width": "6",
            "stroke-miterlimit": "10",
            "cx": "65.1",
            "cy": "65.1",
            "r": "62.1"
        },
        "criss": {
            "tag": "line",
            "classes": ["path", "line"],
            "fill": "none",
            "stroke": "#D06079",
            "stroke-width": "6",
            "stroke-linecap": "round",
            "stroke-miterlimit": "10",
            "x1": "34.4",
            "y1": "37.9",
            "x2": "95.8",
            "y2": "92.3"
        },
        "cross": {
            "tag": "line",
            "classes": ["path", "line"],
            "fill": "none",
            "stroke": "#D06079",
            "stroke-width": "6",
            "stroke-linecap": "round",
            "stroke-miterlimit": "10",
            "x1": "95.8",
            "y1": "38",
            "x2": "34.4",
            "y2": "92.2"
        }}
    }
}